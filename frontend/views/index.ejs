<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
  <% Users.forEach(user => { %>
    <h1><%= user.name %></h1>
    <p><%= user.job %></p>
    <% if(user.age > 18) { %>
    <p><%= user.age %></p>
    <% } %>
 <% }); %>
</body>
</html>